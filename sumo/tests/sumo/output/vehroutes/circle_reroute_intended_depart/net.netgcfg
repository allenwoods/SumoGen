<?xml version="1.0" encoding="UTF-8"?>

<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/netgenerateConfiguration.xsd">

    <spider_network>
        <spider value="true"/>
        <spider.arm-number value="10"/>
        <spider.circle-number value="1"/>
        <spider.space-radius value="1589"/>
        <spider.omit-center value="true"/>
    </spider_network>

    <output>
        <output-file value="net.net.xml"/>
    </output>

    <processing>
        <no-turnarounds value="true"/>
    </processing>

    <building_defaults>
        <default.speed value="19.6"/>
    </building_defaults>

</configuration>
